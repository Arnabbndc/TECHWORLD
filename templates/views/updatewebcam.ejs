<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>


<style>
    .add-form-svk{
        max-width:80vw;
        padding-left: 5vw;
    }
</style>


<form class="add-form-svk" action=<%="/admin/product/updateproduct/" + product.PRODUCTID %> method="post">
  
    
    <div class="form-group">
        
      <label class="form-label" for="disabledInput">Product Type </label>
      <input class="form-control" name="TYPE" id="TYPE" type="text" value="MOTHERBOARD" value="MOTHERBOARD" readonly >

    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Product Name</label>
      <input type="text" class="form-control" name="PRODUCT_NAME" id="PRODUCT_NAME" aria-describedby="emailHelp" value="<%= product.PRODUCT_NAME %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Brand ID</label>
      <input type="text" class="form-control" name="BRANDID" id="BRANDID" aria-describedby="emailHelp" value="<%= product.BRANDID %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Price</label>
      <input type="text" class="form-control" name="PRICE" id="PRICE" aria-describedby="emailHelp" value="<%= product.PRICE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Image</label>
      <input type="text" class="form-control" name="IMAGE" id="IMAGE" aria-describedby="emailHelp" value="<%= product.IMAGE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Warranty</label>
      <input type="text" class="form-control" name="WARRANTY" id="WARRANTY" aria-describedby="emailHelp" value="<%= product.WARRANTY %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Stock</label>
      <input type="text" class="form-control" name="STOCK" id="STOCK" aria-describedby="emailHelp" value="<%= product.STOCK %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Details</label>
      <input type="text" class="form-control" name="DETAILS" id="DETAILS" aria-describedby="emailHelp" value="<%= product.DETAILS %>">
    </div>


    



<div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">WEBCAM_TYPE</label>
    <input type="text" class="form-control" name="WEBCAM_TYPE" id="WEBCAM_TYPE" aria-describedby="emailHelp" value="<%= product.WEBCAM_TYPE %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">RESOLUTION</label>
    <input type="text" class="form-control" name="RESOLUTION" id="RESOLUTION" aria-describedby="emailHelp" value="<%= product.RESOLUTION %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">WEIGHT</label>
    <input type="text" class="form-control" name="WEIGHT" id="WEIGHT" aria-describedby="emailHelp" value="<%= product.WEIGHT %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">MICROPHONE</label>
    <input type="text" class="form-control" name="MICROPHONE" id="MICROPHONE" aria-describedby="emailHelp" value="<%= product.MICROPHONE %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">ZOOM</label>
    <input type="text" class="form-control" name="ZOOM" id="ZOOM" aria-describedby="emailHelp" value="<%= product.ZOOM %>">
    </div>


    <br>
    <input type="submit" class="btn btn-primary" value="UPDATE PRODUCT">
</form>