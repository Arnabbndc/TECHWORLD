<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>


<style>
    .add-form-svk{
        max-width:80vw;
        padding-left: 5vw;
    }
</style>


<form class="add-form-svk" action=<%="/admin/brand/updateBrand/" + brand.BRANDID %> method="post">
  
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Brand Name</label>
      <input  type="text" class="form-control" name="name" id="name"   value="<%= brand.BRAND_NAME %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Logo</label>
      <input  type="text" class="form-control" name="logo" id="logo"   value="<%= brand.LOGO %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Country</label>
      <input  type="text" class="form-control" name="country" id="country"   value="<%= brand.COUNTRY %>">
    </div>  



    <br>
    <input type="submit" class="btn btn-primary" value="ADD BRAND">
</form>

