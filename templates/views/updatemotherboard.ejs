<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>


<style>
    .add-form-svk{
        max-width:80vw;
        padding-left: 5vw;
    }
</style>


<form class="add-form-svk" action=<%="/admin/product/updateproduct/" + product.PRODUCTID %>  method="post">
  
    
    <div class="form-group">
        
      <label class="form-label" for="disabledInput">Product Type </label>
      <input class="form-control" name="TYPE" id="TYPE" type="text" value="MOTHERBOARD" placeholder="MOTHERBOARD" readonly >

    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Product Name</label>
      <input type="text" class="form-control" name="PRODUCT_NAME" id="PRODUCT_NAME" aria-describedby="emailHelp" placeholder="<%= product.PRODUCT_NAME %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Brand ID</label>
      <input type="text" class="form-control" name="BRANDID" id="BRANDID" aria-describedby="emailHelp" placeholder="<%= product.BRANDID %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Price</label>
      <input type="text" class="form-control" name="PRICE" id="PRICE" aria-describedby="emailHelp" placeholder="<%= product.PRICE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Image</label>
      <input type="text" class="form-control" name="IMAGE" id="IMAGE" aria-describedby="emailHelp" placeholder="<%= product.IMAGE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Warranty</label>
      <input type="text" class="form-control" name="WARRANTY" id="WARRANTY" aria-describedby="emailHelp" placeholder="<%= product.WARRANTY %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Stock</label>
      <input type="text" class="form-control" name="STOCK" id="STOCK" aria-describedby="emailHelp" placeholder="<%= product.STOCK %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Details</label>
      <input type="text" class="form-control" name="DETAILS" id="DETAILS" aria-describedby="emailHelp" placeholder="<%= product.DETAILS %>">
    </div>


    <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">CHIPSET</label>
        <input type="text" class="form-control" name="CHIPSET" id="CHIPSET" aria-describedby="emailHelp" placeholder="<%= product.CHIPSET %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">FORM_FACTOR</label>
        <input type="text" class="form-control" name="FORM_FACTOR" id="FORM_FACTOR" aria-describedby="emailHelp" placeholder="<%= product.FORM_FACTOR %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">AUDIO</label>
        <input type="text" class="form-control" name="AUDIO" id="AUDIO" aria-describedby="emailHelp" placeholder="<%= product.AUDIO %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">PCI</label>
        <input type="text" class="form-control" name="PCI" id="PCI" aria-describedby="emailHelp" placeholder="<%= product.PCI %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">SUPPORTED_SOFTWARE</label>
        <input type="text" class="form-control" name="SUPPORTED_SOFTWARE" id="SUPPORTED_SOFTWARE" aria-describedby="emailHelp" placeholder="<%= product.SUPPORTED_SOFTWARE %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">SLOTS</label>
        <input type="text" class="form-control" name="SLOTS" id="SLOTS" aria-describedby="emailHelp" placeholder="<%= product.SLOTS %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">SUPPORTED_MEMORY</label>
        <input type="text" class="form-control" name="SUPPORTED_MEMORY" id="SUPPORTED_MEMORY" aria-describedby="emailHelp" placeholder="<%= product.SUPPORTED_MEMORY %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">MAXIMUM_MEMORY</label>
        <input type="text" class="form-control" name="MAXIMUM_MEMORY" id="MAXIMUM_MEMORY" aria-describedby="emailHelp" placeholder="<%= product.MAXIMUM_MEMORY %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">GRAPHICS</label>
        <input type="text" class="form-control" name="GRAPHICS" id="GRAPHICS" aria-describedby="emailHelp" placeholder="<%= product.GRAPHICS %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">HDMI</label>
        <input type="text" class="form-control" name="HDMI" id="HDMI" aria-describedby="emailHelp" placeholder="<%= product.HDMI %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">USB_TYPE</label>
        <input type="text" class="form-control" name="USB_TYPE" id="USB_TYPE" aria-describedby="emailHelp" placeholder="<%= product.USB_TYPE %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">LAN_PORTS</label>
        <input type="text" class="form-control" name="LAN_PORTS" id="LAN_PORTS" aria-describedby="emailHelp" placeholder="<%= product.LAN_PORTS %>">
        </div>



    <br>
    <input type="submit" class="btn btn-primary" value="UPDATE PRODUCT">
</form>