<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>


<style>
    .add-form-svk{
        max-width:80vw;
        padding-left: 5vw;
    }
</style>


<form class="add-form-svk" action=<%="/admin/product/updateproduct/" + product.PRODUCTID %> method="post">
  
    
    <div class="form-group">
        
      <label class="form-label" for="disabledInput">Product Type </label>
      <input class="form-control" name="TYPE" id="TYPE" type="text" value="MOTHERBOARD" placeholder="MOTHERBOARD" readonly >

    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Product Name</label>
      <input type="text" class="form-control" name="PRODUCT_NAME" id="PRODUCT_NAME" aria-describedby="emailHelp" placeholder="<%= product.PRODUCT_NAME %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Brand ID</label>
      <input type="text" class="form-control" name="BRANDID" id="BRANDID" aria-describedby="emailHelp" placeholder="<%= product.BRANDID %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Price</label>
      <input type="text" class="form-control" name="PRICE" id="PRICE" aria-describedby="emailHelp" placeholder="<%= product.PRICE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Image</label>
      <input type="text" class="form-control" name="IMAGE" id="IMAGE" aria-describedby="emailHelp" placeholder="<%= product.IMAGE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Warranty</label>
      <input type="text" class="form-control" name="WARRANTY" id="WARRANTY" aria-describedby="emailHelp" placeholder="<%= product.WARRANTY %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Stock</label>
      <input type="text" class="form-control" name="STOCK" id="STOCK" aria-describedby="emailHelp" placeholder="<%= product.STOCK %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Details</label>
      <input type="text" class="form-control" name="DETAILS" id="DETAILS" aria-describedby="emailHelp" placeholder="<%= product.DETAILS %>">
    </div>

    


<div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">EFFICIENCY</label>
    <input type="text" class="form-control" name="EFFICIENCY" id="EFFICIENCY" aria-describedby="emailHelp" placeholder="<%= product.EFFICIENCY %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">CERTIFICATION</label>
    <input type="text" class="form-control" name="CERTIFICATION" id="CERTIFICATION" aria-describedby="emailHelp" placeholder="<%= product.CERTIFICATION %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">FAN_SIZE</label>
    <input type="text" class="form-control" name="FAN_SIZE" id="FAN_SIZE" aria-describedby="emailHelp" placeholder="<%= product.FAN_SIZE %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">MODULAR_TYPE</label>
    <input type="text" class="form-control" name="MODULAR_TYPE" id="MODULAR_TYPE" aria-describedby="emailHelp" placeholder="<%= product.MODULAR_TYPE %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">TOTAL_POWER</label>
    <input type="text" class="form-control" name="TOTAL_POWER" id="TOTAL_POWER" aria-describedby="emailHelp" placeholder="<%= product.TOTAL_POWER %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">PEAK_POWER</label>
    <input type="text" class="form-control" name="PEAK_POWER" id="PEAK_POWER" aria-describedby="emailHelp" placeholder="<%= product.PEAK_POWER %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">AC_INPUT</label>
    <input type="text" class="form-control" name="AC_INPUT" id="AC_INPUT" aria-describedby="emailHelp" placeholder="<%= product.AC_INPUT %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">DC_OUTPUT</label>
    <input type="text" class="form-control" name="DC_OUTPUT" id="DC_OUTPUT" aria-describedby="emailHelp" placeholder="<%= product.DC_OUTPUT %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">MAIN_POWER</label>
    <input type="text" class="form-control" name="MAIN_POWER" id="MAIN_POWER" aria-describedby="emailHelp" placeholder="<%= product.MAIN_POWER %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">CPU</label>
    <input type="text" class="form-control" name="CPU" id="CPU" aria-describedby="emailHelp" placeholder="<%= product.CPU %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">PCI_E</label>
    <input type="text" class="form-control" name="PCI_E" id="PCI_E" aria-describedby="emailHelp" placeholder="<%= product.PCI_E %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">SATA</label>
    <input type="text" class="form-control" name="SATA" id="SATA" aria-describedby="emailHelp" placeholder="<%= product.SATA %>">
    </div>
    <div class="form-group">
    <label for="exampleInputEmail1" class="form-label mt-4">MOLEX</label>
    <input type="text" class="form-control" name="MOLEX" id="MOLEX" aria-describedby="emailHelp" placeholder="<%= product.MOLEX %>">
    </div>   
    
    

    <br>
    <input type="submit" class="btn btn-primary" value="UPDATE PRODUCT">
</form>