<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>


<style>
    .add-form-svk{
        max-width:80vw;
        padding-left: 5vw;
    }
</style>

<%- include('newheader.ejs') %>

<form class="add-form-svk" action="/admin/product/addproduct/motherboard" method="post">
  
    
    <div class="form-group">
        
            <label class="form-label" for="disabledInput">Product Type </label>
            <input class="form-control" name="TYPE" id="TYPE" type="text" value="MOTHERBOARD" placeholder="MOTHERBOARD" readonly >
      
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Product Name</label>
      <input  type="text" class="form-control" name="PRODUCT_NAME" id="PRODUCT_NAME"   placeholder="Enter product name">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Brand ID</label>
      <input  type="text" class="form-control" name="BRANDID" id="BRANDID"   placeholder="Enter brand ID">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Price</label>
      <input  type="text" class="form-control" name="PRICE" id="PRICE"   placeholder="Enter price">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Image</label>
      <input  type="text" class="form-control" name="IMAGE" id="IMAGE"   placeholder="Enter image link">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Warranty</label>
      <input  type="text" class="form-control" name="WARRANTY" id="WARRANTY"   placeholder="Enter warranty details">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Stock</label>
      <input  type="text" class="form-control" name="STOCK" id="STOCK"   placeholder="Enter stock quantity">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Details</label>
      <input  type="text" class="form-control" name="DETAILS" id="DETAILS"   placeholder="Enter product details">
    </div>


    



    <br>
    <input type="submit" class="btn btn-primary" value="ADD PRODUCT">
</form>

