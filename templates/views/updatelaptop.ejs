<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>


<style>
    .add-form-svk{
        max-width:80vw;
        padding-left: 5vw;
    }
</style>


<form class="add-form-svk" action=<%="/admin/product/updateproduct/" + product.PRODUCTID %> method="post">
  
    
    <div class="form-group">
        
      <label class="form-label" for="disabledInput">Product Type </label>
      <input class="form-control" name="TYPE" id="TYPE" type="text" value="MOTHERBOARD" placeholder="MOTHERBOARD" readonly >

    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Product Name</label>
      <input type="text" class="form-control" name="PRODUCT_NAME" id="PRODUCT_NAME" aria-describedby="emailHelp" placeholder="<%= product.PRODUCT_NAME %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Brand ID</label>
      <input type="text" class="form-control" name="BRANDID" id="BRANDID" aria-describedby="emailHelp" placeholder="<%= product.BRANDID %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Price</label>
      <input type="text" class="form-control" name="PRICE" id="PRICE" aria-describedby="emailHelp" placeholder="<%= product.PRICE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Image</label>
      <input type="text" class="form-control" name="IMAGE" id="IMAGE" aria-describedby="emailHelp" placeholder="<%= product.IMAGE %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Warranty</label>
      <input type="text" class="form-control" name="WARRANTY" id="WARRANTY" aria-describedby="emailHelp" placeholder="<%= product.WARRANTY %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Stock</label>
      <input type="text" class="form-control" name="STOCK" id="STOCK" aria-describedby="emailHelp" placeholder="<%= product.STOCK %>">
    </div>
    
    <div class="form-group">
      <label for="exampleInputEmail1" class="form-label mt-4">Details</label>
      <input type="text" class="form-control" name="DETAILS" id="DETAILS" aria-describedby="emailHelp" placeholder="<%= product.DETAILS %>">
    </div>


    <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">DISPLAY</label>
        <input type="text" class="form-control" name="DISPLAY" id="DISPLAY" aria-describedby="emailHelp" placeholder="<%= product.DISPLAY %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">GRAPHICS</label>
        <input type="text" class="form-control" name="GRAPHICS" id="GRAPHICS" aria-describedby="emailHelp" placeholder="<%= product.GRAPHICS %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">STORAGE</label>
        <input type="text" class="form-control" name="STORAGE" id="STORAGE" aria-describedby="emailHelp" placeholder="<%= product.STORAGE %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">MEMORY</label>
        <input type="text" class="form-control" name="MEMORY" id="MEMORY" aria-describedby="emailHelp" placeholder="<%= product.MEMORY %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">PROCESSOR</label>
        <input type="text" class="form-control" name="PROCESSOR" id="PROCESSOR" aria-describedby="emailHelp" placeholder="<%= product.PROCESSOR %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">ADAPTER</label>
        <input type="text" class="form-control" name="ADAPTER" id="ADAPTER" aria-describedby="emailHelp" placeholder="<%= product.ADAPTER %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">BATTERY</label>
        <input type="text" class="form-control" name="BATTERY" id="BATTERY" aria-describedby="emailHelp" placeholder="<%= product.BATTERY %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">OPERATING_SYSTEM</label>
        <input type="text" class="form-control" name="OPERATING_SYSTEM" id="OPERATING_SYSTEM" aria-describedby="emailHelp" placeholder="<%= product.OPERATING_SYSTEM %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">NETWORKING</label>
        <input type="text" class="form-control" name="NETWORKING" id="NETWORKING" aria-describedby="emailHelp" placeholder="<%= product.NETWORKING %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">KEYBOARD</label>
        <input type="text" class="form-control" name="KEYBOARD" id="KEYBOARD" aria-describedby="emailHelp" placeholder="<%= product.KEYBOARD %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">BLUETOOTH</label>
        <input type="text" class="form-control" name="BLUETOOTH" id="BLUETOOTH" aria-describedby="emailHelp" placeholder="<%= product.BLUETOOTH %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">AUDIO</label>
        <input type="text" class="form-control" name="AUDIO" id="AUDIO" aria-describedby="emailHelp" placeholder="<%= product.AUDIO %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">WEBCAM</label>
        <input type="text" class="form-control" name="WEBCAM" id="WEBCAM" aria-describedby="emailHelp" placeholder="<%= product.WEBCAM %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">PORTS_CONNECTORS</label>
        <input type="text" class="form-control" name="PORTS_CONNECTORS" id="PORTS_CONNECTORS" aria-describedby="emailHelp" placeholder="<%= product.PORTS_CONNECTORS %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">COLOR</label>
        <input type="text" class="form-control" name="COLOR" id="COLOR" aria-describedby="emailHelp" placeholder="<%= product.COLOR %>">
        </div>
        <div class="form-group">
        <label for="exampleInputEmail1" class="form-label mt-4">WEIGHT</label>
        <input type="text" class="form-control" name="WEIGHT" id="WEIGHT" aria-describedby="emailHelp" placeholder="<%= product.WEIGHT %>">
        </div>




    <br>
    <input type="submit" class="btn btn-primary" value="UPDATE PRODUCT">
</form>