<style>
    .pc-buildersvk-card{
        width:50vw;
        height: 15vh;
    }
    
    .img {
    float: left;
    width: 7vw;
    height: 7vh;
    }
    
    .txt {
    float: left;
    max-width: 30vw;
    max-height: 20vh;
    padding-left: 3vw;
    padding-top: 1vh;
    font-size: 1rem;
    }
    
    .pc-builder-btn{
        float: right;
    width:10vw;
    height: 5vh;
     border-radius: 1.5vw;
     cursor: pointer;
     font-size: 1rem;
    }
    
    .ok{
        margin-bottom: 5vh;
    }

    .price{
        float: left;
        max-width: 30vw;
        padding-left: 20vw;
        max-height: 20vh;
        padding-top: 1vh;
        font-size: 1rem;
    }

    .pc-builder-btn-cart{
        float: left;
        margin-left: 12vw;
        margin-bottom: 5vw;
        width:5vw;
        height: 10vh;
        cursor: pointer;
        font-size: 1rem;

    }
    
</style>
 
